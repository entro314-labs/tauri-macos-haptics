{
  "name": "tauri-macos-haptics-api",
  "version": "2.0.0",
  "author": "Entro314 Labs",
  "contributors": ["ItsEeleeya"],
  "license": "MIT",
  "keywords": [
    "tauri",
    "tauri-plugin",
    "haptics",
    "macos",
    "taptic-engine",
    "force-touch"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/entro314-labs/tauri-macos-haptics.git"
  },
  "description": "Modern macOS haptics (Taptic Engine) plugin for Tauri v2 apps with objc2 bindings.",
  "type": "module",
  "types": "./dist-js/index.d.ts",
  "main": "./dist-js/index.cjs",
  "module": "./dist-js/index.js",
  "exports": {
    "types": "./dist-js/index.d.ts",
    "import": "./dist-js/index.js",
    "require": "./dist-js/index.cjs"
  },
  "files": [
    "dist-js",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "dev": "pnpm run build && cd example && pnpm tauri dev",
    "example": "pnpm run build && cd example && pnpm tauri dev",
    "example:build": "pnpm run build && cd example && pnpm tauri build"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.9.1"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.3.0",
    "@tailwindcss/postcss": "^4.1.18",
    "rollup": "^4.57.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  }
}
